{
  "name": "next-codegen",
  "description": "Next.js All-in-One starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "init": "prisma init",
    "migrate": "prisma migrate dev --preview-feature",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json graphql/schema",
    "instrospect": "prisma instrospect",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "graphql": "^15.5.0",
    "graphql-codegen-plugin-typescript-swr": "^0.6.1",
    "graphql-request": "^3.4.0",
    "next": "^10.0.9",
    "next-themes": "^0.0.12",
    "preact": "^10.5.13",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.0.0-rc.3",
    "react-query": "^3.12.2",
    "swr": "^0.5.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-graphql-request": "^3.1.0",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@graphql-codegen/typescript-react-query": "^1.2.4",
    "@tailwindcss/forms": "^0.2.1",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "author": {
    "name": "Max Aiwon",
    "email": "max@aiwon.org",
    "url": "https://aiwon.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comingAlive/next-tailwind.git"
  },
  "license": "MIT"
}
